{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"outDir": "../../dist/out-tsc",
		"module": "commonjs",
		"moduleResolution": "node10",
		"types": ["jest", "node"],
		"jsx": "react-jsx"
	},
	"include": [
		"jest.config.ts",
		"jest.config.cts",
		"src/**/*.test.ts",
		"src/**/*.spec.ts",
		"src/**/*.test.tsx",
		"src/**/*.spec.tsx",
		"src/**/*.d.ts"
	]
}
